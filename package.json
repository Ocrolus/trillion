{
  "name": "trillion",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-commonjs": "babel --presets es2015 src --out-dir lib",
    "build-react": "browserify -s Trillion . -o dist/trillion.js",
    "build": "npm run build-react",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "add-module-exports"
          ]
        }
      ],
      [
        "browserify-shim"
      ]
    ]
  },
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imsky/trillion.git"
  },
  "keywords": [
    "table",
    "million",
    "rows",
    "react"
  ],
  "author": "Ivan Malopinsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imsky/trillion/issues"
  },
  "homepage": "https://github.com/imsky/trillion#readme",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.1.1",
    "immutable": "^3.7.5",
    "react": "^0.14",
    "react-dom": "^0.14"
  },
  "devDependencies": {
    "babel-cli": "^6",
    "babel-preset-es2015": "^6",
    "babel-preset-react": "^6",
    "babelify": "^7",
    "browserify": "^12",
    "browserify-shim": "^3"
  }
}
